<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reception">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_3"
        android:label="Reception"
        android:roundIcon="@mipmap/ic_launcher_3_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity
            android:name="com.example.reception.Dashboard"
            android:label="Dashboard"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.reception.RegisterHost"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name="com.example.reception.AllHost"
            android:screenOrientation="sensorPortrait" />

        <activity
            android:name="com.example.reception.VisitorOut"
            android:screenOrientation="sensorPortrait" />


    </application>

</manifest>